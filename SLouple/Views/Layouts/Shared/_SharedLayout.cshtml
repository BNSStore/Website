@{
    Lang lang = ViewBag.lang;
}
@{
    lang.AddKeyword(ViewBag.title);
    @RenderSection("lang", required: false);
    lang.GetTranslationFromSQL();
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    @if (IsSectionDefined("description")) {
        <meta name="description" content="@RenderSection("description", required: false).ToString().Trim()">
    }
    @RenderSection("meta", required: false)
    <title>@lang.GetTrans(@ViewBag.title)</title>
    @if (IsSectionDefined("icon"))
    {
        @RenderSection("icon", required: false)
    }
    else
    {
        <link rel="apple-touch-icon" href="//shared.bnsstore.com/res/Images/Logo/Logo/png" />
        <link rel="shortcut icon" href="//shared.bnsstore.com/res/Images/Logo/Logo/png">
        <link rel="icon" href="//shared.bnsstore.com/res/Images/Logo/Logo/png">
    }
    <link rel="stylesheet" href="//shared.bnsstore.com/res/CSS/normalize/3.0.2.min/css">
    <link rel="stylesheet" href="//shared.bnsstore.com/res/CSS/jQueryUI/1.11.2.min/css">

    <script src="//shared.bnsstore.com/res/JS/jQuery/2.1.3.min/js"></script>
    <script src="//shared.bnsstore.com/res/JS/jQueryAnimateShadow/0.0.1.min/js"></script>
    <script src="//shared.bnsstore.com/res/JS/jQueryMobile/1.4.5.min/js"></script>
    <script src="//shared.bnsstore.com/res/JS/jQueryUI/1.11.4.min/js"></script>
    <script src="//shared.bnsstore.com/res/JS/jQueryUITouchPunch/0.2.3.min/js"></script>
    <script src="//shared.bnsstore.com/res/JS/Velocity/1.2.2.min/js"></script>
    <script src="//shared.bnsstore.com/res/JS/AutoTextSize/0.0.2.min/js"></script>
    @RenderSection("head", required: false)
</head>
<body>
    @RenderSection("body", required: true)
    <script>
    (function (i, s, o, g, r, a, m) {
        i['GoogleAnalyticsObject'] = r; i[r] = i[r] || function () {
            (i[r].q = i[r].q || []).push(arguments)
        }, i[r].l = 1 * new Date(); a = s.createElement(o),
        m = s.getElementsByTagName(o)[0]; a.async = 1; a.src = g; m.parentNode.insertBefore(a, m)
    })(window, document, 'script', '//www.google-analytics.com/analytics.js', 'ga');

    ga('create', 'UA-58520133-1', 'auto');
    ga('send', 'pageview');

    </script>
</body>
</html>